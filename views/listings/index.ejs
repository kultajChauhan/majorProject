<% layout("layouts/boilerplate") %>
<body>
    <h2>All listings</h2>
    <form action="/listings/new" method="get" >
        <button>Create New Listing!</button>
    </form>
    <ul>
        <% for(let listing of Listings){%>
            <li>
                <a href="/listing/<%=listing._id%>">
                    <%=listing.title%>
                </a>
            </li>
            <% }%>
    </ul>


</body>